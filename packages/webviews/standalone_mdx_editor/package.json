{
  "name": "standalone_mdx_editor",
  "version": "0.0.0",
  "keywords": [
    "academic",
    "fluster",
    "note taking",
    "science",
    "STEM"
  ],
  "funding": [
    {
      "type": "paypal",
      "url": "https://www.paypal.com/ncp/payment/D6S6NP4AHJD6Y"
    },
    {
      "type": "patreon",
      "url": "https://www.patreon.com/uhlittlelessdum/about"
    },
    {
      "type": "github",
      "url": "https://github.com/flusterIO/fluster"
    }
  ],
  "type": "module",
  "private": true,
  "scripts": {
    "build": "wireit",
    "build:ipad": "wireit",
    "build:mac": "wireit",
    "dev": "vite dev",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "wireit": {
    "build:mac": {
      "command": "tsc -b && vite build -c ./vite_mac.config.ts"
    },
    "build:ipad": {
      "command": "tsc -b && vite build -c ./vite_ipad.config.ts"
    },
    "build": {
      "dependencies": [
        "build:ipad",
        "build:mac"
      ]
    }
  },
  "dependencies": {
    "@fluster/webview_utils": "workspace:*",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@fluster/shared_config": "workspace:*",
    "@types/node": "24.10.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.1",
    "eslint": "9.39.1",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "globals": "16.5.0",
    "typescript": "*",
    "typescript-eslint": "8.46.4",
    "vite": "7.2.4"
  },
  "license": "UNLICENSED"
}
