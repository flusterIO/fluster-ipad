#!/bin/sh
# This will clear the dist directory in the iOS app if there is one from a previous build
rm -rf "${BUILT_PRODUCTS_DIR}/${PRODUCT_NAME}.app/dist"

cd ../../packages/webview_utils

~/.pnpm/pnpm install
~/.pnpm/pnpm build

cd ../webviews/splitview_mdx_editor

~/.pnpm/pnpm install
~/.pnpm/pnpm run build

cd ../bibtex_editor_webview

~/.pnpm/pnpm install
~/.pnpm/pnpm run build
